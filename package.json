{
  "name": "pnpm-rollup-ts-template",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.cjs",
  "umd": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "nodemon",
    "lint": "standard ./src/**/*.ts ./__tests__/**/*.ts",
    "dev": "rollup -c rollup.config.ts --configPlugin esbuild -w",
    "test": "pnpm run lint && jest --config=jest.config.ts --passWithNoTests",
    "test::coverage": "jest --coverage --config=jest.config.ts",
    "build::clean": "rimraf ./dist",
    "build::proc": "rollup -c rollup.config.ts --configPlugin esbuild",
    "build::types": "pnpm build::clean && tsc --project ./tsconfig.json --declaration true",
    "build": "pnpm build::clean && pnpm build::proc",
    "publish::major": "npm version major",
    "publish::minor": "npm version minor",
    "publish::patch": "npm version patch",
    "publish::test": "npx yalc publish",
    "docs:clean": "rimraf ./docs",
    "docs::geneator": "typedoc",
    "docs": "pnpm docs:clean && pnpm docs::geneator"
  },
  "keywords": [
    "pnpm",
    "rollup",
    "typescript",
    "template"
  ],
  "author": "ishiko",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/seedrandom": "^3.0.8",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "standard": "^17.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.13",
    "typedoc-plugin-extras": "^3.0.0",
    "typedoc-plugin-katex": "^0.1.2",
    "typescript": "^5.4.5"
  }
}